/dts-v1/;
/plugin/;

&cm33 {
	status = "okay";
};

/* NPU shared region */
&ethosu_mem {
	status = "okay";
};

/* disabled per default, console for M33 */
&lpuart3 {
	status = "disabled";
};

/* carveout for Cortex M33 running in DDR, disabled by default */
&m33_reserved {
	status = "okay";
};

/* mailbox to communicate with Cortex M33 */
&mu1 {
	status = "okay";
};

/* NPU only usable with Cortex M33 */
&npu {
	status = "okay";
};

&rsc_table {
	status = "okay";
};

/* RPMSG interface for Cortex M33, disabled by default */
&vdev0vring0 {
	status = "okay";
};

&vdev0vring1 {
	status = "okay";
};

&vdev1vring0 {
	status = "okay";
};

&vdev1vring1 {
	status = "okay";
};

&vdevbuffer {
	status = "okay";
};
