&mcu_i2c0 {
	pinctrl-names = "default";
	pinctrl-0 = <&mcu_i2c_pins_default>;
	clock-frequency = <400000>;

	pca9555_D24: gpio@20 {
		compatible = "nxp,pca9555";
		reg = <0x20>;
		gpio-controller;
		#gpio-cells = <2>;
	};

	pca9555_D25: gpio@21 {
		compatible = "nxp,pca9555";
		reg = <0x21>;
		gpio-controller;
		#gpio-cells = <2>;
	};

	ina226@40 {
		compatible = "ti,ina226";
		reg = <0x40>;
		shunt-resistor = <3>;
	};

	ina226@45 {
		compatible = "ti,ina226";
		reg = <0x45>;
		shunt-resistor = <3>;
	};
};


&pca9555_D24 {
	p0 {
		/* P0 - PRG0_RGMII_RST#_AUDIO_RST# */
		gpio-hog;
		gpios = <0x00 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "PRG0_RGMII_RST#_AUDIO_RST#";
	};
	p2 {
		/* P2 - GPIOEXP0_IO0_3_PRG0_ETH_EN# */
		gpio-hog;
		gpios = <0x02 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "GPIOEXP0_IO0_3_PRG0_ETH_EN#";
	};
	p3 {
		/* P3 - PRG1_RGMII_RST# */
		gpio-hog;
		gpios = <0x03 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "PRG1_RGMII_RST#";
	};
	p5 {
		/* P5 - PRG1_ETH_EN# */
		gpio-hog;
		gpios = <0x05 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "PRG1_ETH_EN#";
	};
	p7 {
		/* P7 - PRG2_RGMII_RST# */
		gpio-hog;
		gpios = <0x07 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "PRG2_RGMII_RST#";
	};
	p9 {
		/* P9 - MCU_RGMII0_RST# */
		gpio-hog;
		gpios = <0x09 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "MCU_RGMII0_RST#";
	};
};

&pca9555_D25 {
	p0 {
		/* P0 - MPCIE_1V5_EN*/
		gpio-hog;
		gpios = <0x00 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "MPCIE_1V5_EN";
	};
	p1 {
		/* P1 - DISPLAY_3V3_EN*/
		gpio-hog;
		gpios = <0x01 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "DISPLAY_3V3_EN";
	};
	p3 {
		/* P3 - LCD_PWR_EN*/
		gpio-hog;
		gpios = <0x03 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "LCD_PWR_EN";
	};
	p4 {
		/* P4 - LCD_BLT_EN*/
		gpio-hog;
		gpios = <0x04 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "LCD_BLT_EN";
	};
	p5 {
		/* P5 - DISPLAY_RST#*/
		gpio-hog;
		gpios = <0x05 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "DISPLAY_RST#";
	};
	p6 {
		/* P6 - USB_HUB_RST# */
		gpio-hog;
		gpios = <0x06 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "USB_HUB_RST#";
	};
	p7 {
		/* P7 - CLK_PDN# */
		gpio-hog;
		gpios = <0x07 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "CLK_PDN#";
	};

	p9 {
		/* P9 - MPCIE_DIS#*/
		gpio-hog;
		gpios = <0x09 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "MPCIE_DIS#";
	};
};


